[Unit]
Description=Whoami test container
After=traefik.service
Requires=traefik.service

[Container]
Image=docker.io/traefik/whoami:latest
Network=traefik.network
AutoUpdate=registry
Label=traefik.enable=true
Label=traefik.http.routers.whoami.rule=Host(`whoami.jarz.casa`)
Label=traefik.http.routers.whoami.entrypoints=web
Label=traefik.http.services.whoami.loadbalancer.server.port=80

[Service]
Restart=always
TimeoutStartSec=900

[Install]
WantedBy=default.target

